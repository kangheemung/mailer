<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for @user,url: users_create_path do |f| %>
<%= f.label :name %>
 <%= f.text_field :name %></br>
<%= f.label :email%>
 <%= f.text_field :email, class: "class_name", maxlength: 40 %></br>
 <%= f.label :password%>
 <%= f.password_field :password, class: "class_name", maxlength: 40 %></br>
  <%= f.label :password_confirmation%>
 <%= f.password_field :password_confirmation, class: "class_name", maxlength: 40 %></br>
<%= f.submit "click "%>
<% end %>
